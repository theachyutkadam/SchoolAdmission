<%=form_for @address do |f|%>
  <% if @address.errors.any? %>
    <div id="error_explanation">
      <h2>
        <%= pluralize(@address.errors.count, "error") %> prohibited
        this address from being saved:
      </h2>
      <ul>
        <% @address.errors.full_messages.each do |msg| %>
          <li><%= msg %></li>
        <% end %>
      </ul>
    </div>
  <% end %>

  Country:- <%=f.collection_select :country_id, Country.all, :id, :name%><br><br>
  State:- <%=f.collection_select :state_id, State.all, :id, :name%><br><br>
  District:- <%=f.collection_select :district_id, District.all, :id, :name%><br><br>
  Taluka:- <%=f.collection_select :taluka_id, Taluka.all, :id, :name%><br><br>
  <%=f.hidden_field :user_id, value: @user.id%><br><br>
  <%=f.text_field :pincode, placeholder: "Pincode"%><br><br>
  <%=f.submit%>
  <%=link_to "Back", teachers_path%>
<%end%>

<%= render 'index'%>